{
  "name": "magick.wasm",
  "version": "1.0.0",
  "description": "The WASM library for ImageMagick",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand",
    "lint": "eslint src tests --ext .ts",
    "demo": "tsc && node lib/demo.js",
    "prepare": "cpy ./node_modules/magick.native/wasm/magick-Q8.js ./lib/wasm --rename=magick.js && cpy ./node_modules/magick.native/wasm/magick-Q8.d.ts ./lib/wasm --rename=magick.d.ts && cpy ./node_modules/magick.native/wasm/magick-Q8.js ./src/wasm --rename=magick.js && cpy ./node_modules/magick.native/wasm/magick-Q8.d.ts ./src/wasm --rename=magick.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlemstra/Magick.WASM.git"
  },
  "author": "Dirk Lemstra",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dlemstra/Magick.WASM/issues"
  },
  "homepage": "https://github.com/dlemstra/Magick.WASM",
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "cpy-cli": "~2.0.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "magick.native": "https://dl.dropboxusercontent.com/s/i0zh9tp7lsji28k/magick.native-0.201910.270525.tgz",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  }
}
